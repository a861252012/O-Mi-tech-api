<meta http-equiv="pragma" content="no-cache">
<meta http-equiv="cache-control" content="no-cache">
<meta http-equiv="expires" content="0">

<style type="text/css">
body{margin:0 auto;padding:0;text-align:center;color:#000000;font-size:13px;background:#fff;width:100%;height:100%;}
div,p,a,span{margin:0;padding:0;color:#000000;font-size:13px;}
a{text-decoration:none;color:#000000;}
 div,p{margin:0;padding:0;}       
 object:focus { outline:none; }
a{ text-decoration: none;}
.btn{
  font-family: 'Microsoft YaHei',sans-serif;
  font-size: 16px;
  line-height: 1.5em;
  display: block;
  min-width: 90px;
  padding: 6px 20px;
  cursor: pointer;
  text-align: center;
  text-decoration: none!important;
  color: #fff;
  border: none;
  border-radius: 50px;
  background-color: #e50080;
  background-image: linear-gradient(to bottom,#f09 0,#ce0068 100%);
  background-repeat: repeat-x;
  box-shadow: 0 2px 3px rgba(255,255,255,.5);
}

.btn:hover{
  opacity: .8;
}
.btn-orange{
  background-image: linear-gradient(to bottom,#ff4e00 0,#ff0100 100%);
  background-repeat: repeat-x;
}

.error_img{
	width: 736px;
	height: 229px;
	margin: 100px auto 0px auto;
}

.error_btn_box{
	width: 736px;
	margin: 0px auto;
}

.error_btn_box .btn{
	float: right;
	margin-left: 20px;
}
</style>
<script type="text/javascript">
function _load(fun){//本函数检测文档是否加载完成，如果完成开始执行fun函数。
     if (window.attachEvent){
         window.attachEvent('onload', fun);
     }else{
         window.addEventListener('load', fun, false);
    }
}
function async_load(src){//本函数执行脚本添加，参数src为脚本资源的地址。
    var live800_script = document.createElement('script');
    live800_script.type = 'text/javascript';
    live800_script.async = true;
    live800_script.src = src;
    document.body.appendChild(live800_script);
}
</script>
<script type="text/javascript">
	_load(function() {
		var site_domain = document.domain;
		window.site_domain = site_domain;
		_img_url = "http://" + site_domain + "/flash";
        {% if flash_ret==1 %}
            _img_url={{ flash_url }}
        {% endif %}
		var path = _img_url + "/image/other/loading_fail.png";
		document.getElementById("error_img_div").style.backgroundImage = "url(" + path + ")";
	});
	</script>
</head>
<body>
<div id="error_img_div" class="error_img">
</div>
<div class="error_btn_box">
	<a href="#" id="rtnIndex" class="btn btn-orange">返回首页</a>
	<a href="javascript:location.href='';" class="btn">重新进入房间</a>
</div>
    <script type="text/javascript">
document.getElementById("rtnIndex").setAttribute('href', 'http://'+location.hostname);
</script>
